   "source": [
    "ecom_sales = df\n",
    "\n",
    "app = dash.Dash()\n",
    "\n",
    "server = app.server" 
    "\n"
    "app.layout = html.Div([\n",
    "  html.H1('Sales breakdowns'),\n",
    "  html.Div(\n",
    "    children=[\n",
    "    html.Div(\n",
    "        children=[\n",
    "        html.H2('Controls'),\n",
    "        html.Br(),\n",
    "        html.H3('Season Select'),\n",
    "        dcc.Dropdown(id='season_dd',\n",
    "        options=[\n",
    "            {'label':'Winter', 'value':'Winter'},\n",
    "            {'label':'Spring', 'value':'Spring'},\n",
    "            {'label':'Summer', 'value':'Summer'},\n",
    "            {'label':'Fall', 'value':'Fall'}],\n",
    "            style={'width':'200px', 'margin':'0 auto'})\n",
    "        ],\n",
    "        style={'width':'350px', 'height':'350px', 'display':'inline-block', 'vertical-align':'top', 'border':'1px solid black', 'padding':'20px'}),\n",
    "    html.Div(children=[\n",
    "            dcc.Graph(id='major_cat'),\n",
    "            html.H2('Major Category', \n",
    "            style={ 'border':'2px solid black', 'width':'200px', 'margin':'0 auto'})\n",
    "            ],\n",
    "             style={'width':'700px','display':'inline-block'}\n",
    "             ),\n",
    "    html.Div(children=[\n",
    "            dcc.Graph(id='gender_pie'),\n",
    "            html.H2('item_purchased_bar', \n",
    "            style={ 'border':'2px solid black', 'width':'200px', 'margin':'0 auto'})\n",
    "            ],\n",
    "             style={'width':'700px','display':'inline-block'}\n",
    "             ),\n",
    "    html.Div(children=[\n",
    "            dcc.Graph(id='item_purchased_bar'),\n",
    "            html.H2('item_purchased_bar', \n",
    "            style={ 'border':'2px solid black', 'width':'200px', 'margin':'0 auto'})\n",
    "            ],\n",
    "             style={'width':'700px','display':'inline-block'}\n",
    "             )                  \n",
    "    ])], \n",
    "  style={'text-align':'center', 'display':'inline-block', 'width':'100%'}\n",
    "  )\n",
    "\n",
    "# Define a callback function to update the graphs\n",
    "@app.callback(\n",
    "    [Output(component_id='major_cat', component_property='figure'),\n",
    "     Output(component_id='gender_pie', component_property='figure'),\n",
    "     Output(component_id='item_purchased_bar', component_property='figure')],\n",
    "    Input(component_id='season_dd', component_property='value')\n",
    ")\n",
    "def update_plot(input_season):\n",
    "    # Set a default value\n",
    "    season_filter = 'All Seasons'\n",
    "    # Ensure the DataFrame is not overwritten\n",
    "    sales = ecom_sales.copy(deep=True)\n",
    "    # Create a conditional to filter the DataFrame if the input exists\n",
    "    if input_season:\n",
    "        season_filter = input_season\n",
    "        sales = sales[sales['Season'] == season_filter]\n",
    "    \n",
    "    # Group and aggregate data for major categories\n",
    "    ecom_bar_major_cat = sales.groupby('Category')['Purchase Amount (USD)'].agg('sum').reset_index(name='Total Sales ($)')\n",
    "\n",
    "    # Group and aggregate data by 'Gender' for a pie chart\n",
    "    gender_pie_data = sales.groupby('Gender')['Purchase Amount (USD)'].agg('sum').reset_index(name='Total Sales ($)')\n",
    "\n",
    "    # Group and aggregate data by 'Item Purchased' for a bar graph\n",
    "    item_purchased_bar_data = sales.groupby('Item Purchased')['Purchase Amount (USD)'].agg('sum').reset_index(name='Total Sales ($)')\n",
    "\n",
    "    # Create a bar chart using Plotly Express for major categories\n",
    "    bar_fig_major_cat = px.bar(\n",
    "        title=f'Sales in {season_filter}',\n",
    "        data_frame=ecom_bar_major_cat,\n",
    "        x='Total Sales ($)',\n",
    "        y='Category',\n",
    "        color='Category',\n",
    "        color_discrete_map={'Clothing': 'blue', 'Footwear': 'red', 'Outerwear': 'green', 'Accessories': 'yellow'}\n",
    "    )\n",
    "\n",
    "    # Create a pie chart for gender distribution\n",
    "    pie_fig_gender = px.pie(\n",
    "        title=f'Gender Distribution in {season_filter}',\n",
    "        data_frame=gender_pie_data,\n",
    "        values='Total Sales ($)',\n",
    "        names='Gender',\n",
    "        hole=0.3 \n",
    "    )\n",
    "    \n",
    "    # Create a bar chart using Plotly Express for item purchased\n",
    "    bar_fig_item_purchased = px.bar(\n",
    "        title=f'Item Purchased in {season_filter}',\n",
    "        data_frame=item_purchased_bar_data,\n",
    "        x='Total Sales ($)',\n",
    "        y='Item Purchased',\n",
    "        color='Item Purchased'\n",
    "    )\n",
    "\n",
    "    # Return the figures\n",
    "    return bar_fig_major_cat, pie_fig_gender, bar_fig_item_purchased\n",
    "\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    app.run_server(debug=True)"
   ]
  }
